<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.loose.fis.sre.controllers.FarmerOrdersController">
   <children>
      <Button fx:id="pendingButton" layoutX="47.0" layoutY="188.0" mnemonicParsing="false" onAction="#handlePendingAction" text="Pending orders">
         <font>
            <Font name="Verdana" size="18.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="historyButton" layoutX="398.0" layoutY="188.0" mnemonicParsing="false" onAction="#handleHistoryAction" text="Order History">
         <font>
            <Font name="Verdana" size="18.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="backButton" layoutX="47.0" layoutY="324.0" mnemonicParsing="false" onAction="#handleBackAction" text="Back">
         <font>
            <Font size="18.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Text fx:id="errorMessage" layoutX="137.0" layoutY="348.0" strokeType="OUTSIDE" strokeWidth="0.0" />
   </children>
</AnchorPane>
