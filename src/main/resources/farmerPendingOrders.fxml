<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<?import javafx.scene.Cursor?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.loose.fis.sre.controllers.FarmerPendingOrdersController">
   <children>
      <Text layoutX="45.0" layoutY="38.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Pending orders">
         <font>
            <Font name="Arial Rounded MT Bold" size="16.0" />
         </font>
      </Text>
      <ListView fx:id="pendingOrders" layoutX="45.0" layoutY="50.0" prefHeight="326.0" prefWidth="505.0" />
      <Button fx:id="backButton" layoutX="505.0" layoutY="21.0" mnemonicParsing="false" onAction="#handleBackAction" text="Back">
         <font>
            <Font name="Verdana" size="12.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Text fx:id="errorMessage" layoutX="45.0" layoutY="391.0" strokeType="OUTSIDE" strokeWidth="0.0" />
   </children>
</AnchorPane>
