            <Cursor fx:constant="TEXT" />
         </cursor></TextField>
      <Text fx:id="availabilityText" layoutX="159.0" layoutY="351.0" strokeType="OUTSIDE" strokeWidth="0.0" />
      <ImageView fitHeight="186.0" fitWidth="200.0" layoutX="370.0" layoutY="121.0">
         <image>
            <Image url="@img/farmerform.png" />
         </image>
      </ImageView>
      <Button fx:id="backButton" layoutX="525.0" layoutY="334.0" mnemonicParsing="false" onAction="#handleBackAction" text="Back">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font name="Verdana" size="12.0" />
         </font>
      </Button>
