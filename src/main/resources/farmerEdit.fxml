<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.loose.fis.sre.controllers.FarmerEditController">
   <children>
      <TextField fx:id="lastName" layoutX="36.0" layoutY="108.0" prefHeight="13.0" prefWidth="134.0">
         <cursor>
            <Cursor fx:constant="TEXT" />
         </cursor></TextField>
      <TextField fx:id="firstName" layoutX="36.0" layoutY="60.0" prefHeight="13.0" prefWidth="134.0">
         <cursor>
            <Cursor fx:constant="TEXT" />
         </cursor></TextField>
      <Label layoutX="36.0" layoutY="41.0" text="First name">
         <font>
            <Font name="Verdana" size="12.0" />
         </font>
      </Label>
      <Label layoutX="36.0" layoutY="89.0" text="Last name">
         <font>
            <Font name="Verdana" size="12.0" />
         </font>
      </Label>
      <Label layoutX="36.0" layoutY="137.0" text="Description">
         <font>
            <Font name="Verdana" size="12.0" />
         </font>
      </Label>
      <Label layoutX="36.0" layoutY="231.0" text="Address">
         <font>
            <Font name="Verdana" size="12.0" />
         </font>
      </Label>
      <TextField fx:id="phone" layoutX="36.0" layoutY="298.0" prefHeight="13.0" prefWidth="134.0">
         <cursor>
            <Cursor fx:constant="TEXT" />
         </cursor></TextField>
      <Label layoutX="36.0" layoutY="279.0" text="Phone">
         <font>
            <Font name="Verdana" size="12.0" />
         </font>
      </Label>
      <Button fx:id="confirmButton" layoutX="238.0" layoutY="334.0" mnemonicParsing="false" onAction="#handleConfirmAction" text="Confirm">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font name="Verdana" size="12.0" />
         </font></Button>
      <Button fx:id="statusButton" layoutX="36.0" layoutY="334.0" mnemonicParsing="false" onAction="#toggleStatus" text="Availability Status">
         <cursor>
            <Cursor fx:constant="HAND" />
